<h1>Look At All Of The Pets</h1>
<ul>
    {{#each petList}}
    <li>
        {{name}}
        <img src="{{imgUrl}}">
        <a href="/pets/{{petId}}">View</a>
        <a href="/pets/edit/{{petId}}">Edit</a>
        <form action="/pets/delete/{{petId}}" method="POST">
            <button type="submit">Delete</button>
        </form>
    </li>
    {{/each}}
</ul>

<a href="/pets/new">Add Another Pet</a>